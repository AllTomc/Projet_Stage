<instance format="XCSP3" type="CSP">
    <variables>
        <var id="a">1..4</var>
        <var id="b">1..4</var>
        <var id="c">1..10</var>
    </variables>
    <constraints>
        <intension>eq(c,mul(a,b))</intension>
        <intension>eq(mod(c, 2), 0)</intension>
    </constraints>
</instance>
